  function CodeToCountry(Code: Word): Cardinal;
  function CodeToLanguage(Code: Word): Cardinal;
  function CodeToZodiac(Code: Byte): Cardinal;

  function CountryToCode(Country: Cardinal): Word;
  function LanguageToCode(Language: Cardinal): Word;
  function ZodiacToCode(Zodiac: Cardinal): Byte;

implementation

{***************************************************************}
function CodeToCountry(Code: Word): Cardinal;
begin
  Result := Code + 4000;
end;

{***************************************************************}
function CodeToLanguage(Code: Word): Cardinal;
begin
  Result := Code + 5000;
end;

{***************************************************************}
function CodeToZodiac(Code: Byte): Cardinal;
begin
  Result := Code + 6000;
end;

{***************************************************************}
function CountryToCode(Country: Cardinal): Word;
begin
  Result := Country - 4000;
end;

{***************************************************************}
function LanguageToCode(Language: Cardinal): Word;
begin
  Result := Language - 5000;
end;

{***************************************************************}
function ZodiacToCode(Zodiac: Cardinal): Byte;
begin
  Result := Zodiac - 6000;
end;

